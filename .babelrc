{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-decorators",
    "transform-async-to-generator",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style":true }] 
  ]
}